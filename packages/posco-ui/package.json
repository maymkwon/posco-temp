{
  "name": "@posco/ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "js:build": "cross-env NODE_ENV=production tsc -p tsconfig.json",
    "build-js:prod": "rimraf ./lib && yarn js:build && cross-env NODE_ENV=production babel src --out-dir lib --copy-files --source-maps --extensions \".ts,.tsx,.js,.jsx,.mjs\"",
    "js:watch": "rimraf ./lib && cross-env NODE_ENV=development concurrently -k -n \"typescript,babel\" -c \"blue.bold,yellow.bold\"  \"tsc -p tsconfig.json --watch\" \"babel src --out-dir lib --source-maps --extensions \".ts,.tsx,.js,.jsx,.mjs\" --copy-files --watch --verbose\"",
    "storybook": "start-storybook -p 8080",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "rimraf .jest-test-results.json && yarn test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o dist/ --quiet"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@material-ui/core": "^4.11.2",
    "@storybook/addon-a11y": "^6.1.14",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-docs": "^6.1.14",
    "@storybook/addon-jest": "^6.1.14",
    "@storybook/addon-knobs": "^6.1.14",
    "@storybook/addon-viewport": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@types/enzyme": "^3.10.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.2",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "dependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7"
  }
}
